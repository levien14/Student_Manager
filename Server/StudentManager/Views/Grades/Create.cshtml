@model StudentManager.Models.Grade

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>Grade</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="GradeName" class="control-label"></label>
                <input asp-for="GradeName" class="form-control" />
                <span asp-validation-for="GradeName" class="text-danger"></span>
            </div>
            
            
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-info" />
                <input asp-action="Index" type="submit" value="Cancel" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
